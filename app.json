{
  "expo": {
    "name": "PhotoHunter",
    "slug": "photo-hunter-app",
    "version": "1.0.0",
    "scheme": "photo-hunter-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "dark",
      "backgroundColor": "#ffffff"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.photohunterapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.photohunterapp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "$EXPO_PUBLIC_MAPBOX_TOKEN"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your camera to let you take reference photos for photo hunts."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "The app accesses your camera to let you take photos for photo hunts."
        }
      ],
      "expo-font",
      "expo-secure-store"
    ]
  }
}
